    "jsonwebtoken": "^9.0.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "prettier": "^3.7.3",
    "rate-limit-redis": "^4.3.1",
    "sanitize-html": "^2.17.0",
    "stream-chat": "^9.27.2",
