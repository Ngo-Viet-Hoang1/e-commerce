
PORT=3000
NODE_ENV=development

# CORS Configuration
# For development, you can use '*' to allow all origins
# For production, specify exact origins separated by commas
# Example: CORS_ORIGIN=https://yourdomain.com,https://www.yourdomain.com
CORS_ORIGIN=http://localhost:3000,http://localhost:5173

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0
# For production, you might want to use Redis connection URL
# REDIS_URL=redis://username:password@hostname:port
# Service Configuration

SERVICE_NAME=api-service

# Logging
LOG_LEVEL=info